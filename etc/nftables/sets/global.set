        http,
        https
    }
}

# BLACK LISTA, IPCIM + IDOZITO ALAPJAN:
set BLACKLIST_SET{
    type ipv4_addr
    flags dynamic, interval, timeout
    elements ={
        10.99.99.0/24 timeout 168h,
        10.99.89.3-10.99.89.11 timeout 168h,
        10.10.10.2 timeout 2m
    }
}

