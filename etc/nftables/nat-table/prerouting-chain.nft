# PREROUTING-CHAIN
    chain prerouting-chain {
        type nat hook prerouting priority filter; policy accept;
            ip saddr @BLACKLIST_SET counter drop \
                comment "BLACKLISTAS IPCIMROL MINDEN DROP"
            iifname $INTERNET_DEV tcp dport { smtp, @MAILSERVER_PORT_SET } dnat to $MAILSERVER_IP comment "NET-rol ERKEZO LEVELEZO PROTOKOLLOK NATOLASA A MAIL SERVERRE"
            iifname $INTERNET_DEV tcp dport 2222 dnat to $MAILSERVER_IP:22 comment "NET-rol A 2222 PORTRA ERKEZO CSOMAGOK NATOLASA A MAIL SERVER 22 PORTJARA " 
}
